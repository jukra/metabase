<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT" follow="true">
      <PatternLayout pattern="[%t] %-5level %logger{36} - %msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Logger name="metabase" level="ERROR"/>
    <Logger name="metabase-enterprise" level="ERROR"/>
    <Logger name="liquibase" level="FATAL"/>
    <Logger name="metabase.sync" level="INFO"/>
    <Logger name="metabase.driver" level="INFO"/>
    <Logger name="metabase.test.data" level="INFO"/>

    <Root level="FATAL">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>
